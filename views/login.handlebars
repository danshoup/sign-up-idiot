  <main class="container container-fluid m-5">
    <div class="text-center mt-5">
        <form class="login-form" style="max-width:480px; margin:auto;">
          <img class="mt-4 mb-4" src="/images/SignUp.png" height="100" alt="Sign Up Logo">
           <h1 class="h3 mb-3 font-weight-normal"></h1>Please Log In</h1>
           <label for="email-login" class="sr-only"></label>
           <input type="email" id="email-login" class="shadow-sm form-control mb-3" placeholder="Email Address" required autofocus >
           <label for="password-login" class="sr-only"></label>
           <input type="password" id="password-login" placeholder="Password" class="shadow-sm form-control mb-3">
           <div class="mt-3">
               <button class="btn btn-md btn-primary btn-block">Sign In</button>
           </div>
       </form>
     </div>
 
  </main>

<script src="/js/login.js"></script>